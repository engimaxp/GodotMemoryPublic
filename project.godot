; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotMemory"
run/main_scene="res://src/main_window.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"
config/windows_native_icon="res://winicon.ico"

[autoload]

Enums="*res://src/util/enums.tscn"
Signals="*res://src/util/signals.tscn"
Global="*res://src/util/global.tscn"
Utils="*res://src/util/utils.tscn"
Db="*res://src/util/db.tscn"
Files="*res://src/util/files.tscn"

[debug]

gdscript/warnings/unassigned_variable=0
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=800

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg", "res://addons/thread_pool/plugin.cfg")

[gui]

theme/default_font_multichannel_signed_distance_field=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations=PackedStringArray("res://localization/en_US.mo", "res://localization/en_US.po", "res://localization/zh_CN.mo", "res://localization/zh_CN.po")
locale/translations_pot_files=PackedStringArray("res://src/ui/popup_proj_edit_menu.tscn", "res://src/main_window.tscn", "res://src/ui/asset_row.tscn", "res://src/ui/engine_row.tscn", "res://src/ui/image_preview_pop_up.tscn", "res://src/ui/popup_asset_edit_menu.tscn", "res://src/ui/popup_engine_edit_menu.tscn", "res://src/ui/popup_tool_edit_menu.tscn")
